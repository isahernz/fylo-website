---
const navItems = [
  {
    title: "Features",
    label: "features",
    url: "/#features",
  },
  {
    title: "Team",
    label: "team",
    url: "/#team",
  },
  {
    title: "Sign In",
    label: "sign-in",
    url: "/",
  },
];
---

<nav>
  <ul class="flex items-center gap-8">
    {
      navItems.map((item) => (
        <li>
          <a
            href={item.url}
            aria-label={item.label}
            class="text-accent-400 transition-colors duration-300 hover:text-white"
          >
            {item.title}
          </a>
        </li>
      ))
    }
  </ul>
</nav>
